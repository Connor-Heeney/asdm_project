LOAD DATA 
INFILE 'regions.sql'
TRUNCATE
CONTINUEIF NEXT(1:1) = '#'
INTO TABLE s2559258.REGIONS
FIELDS TERMINATED BY '|'
TRAILING NULLCOLS (
    REGION_ID INTEGER EXTERNAL,
    NAME CHAR,
    GEOM COLUMN OBJECT (
        SDO_GTYPE INTEGER EXTERNAL,
